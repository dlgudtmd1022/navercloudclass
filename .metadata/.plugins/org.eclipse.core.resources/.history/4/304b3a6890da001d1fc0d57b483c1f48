package poly.instanceof_;

public class Cup {

	private String itemName;
	private int price;
	private String companyName;
	
	public Cup(String itemName, int price,String companyName) {
		this.itemName = itemName;
		this.price = price;
		this.companyName = companyName;		
		this.companyName = "컵회사";		
	}
}
